{
  "name": "@empjs/cdn-vue-router-pinia",
  "version": "3.5.1",
  "description": "empShare runtime",
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "maintainers": ["xuhongbin", "ckken", "doerme", "ron0115"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empjs/emp.git",
    "directory": "packages/cdn-vue-router-pinia"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/vueRouter.production.umd.js",
  "types": "dist/vueRouter.d.ts",
  "scripts": {
    "dev": "pnpm run /^dev:.*/",
    "dev:bundle": "tsup --watch",
