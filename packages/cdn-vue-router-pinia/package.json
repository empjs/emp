    "dev-rs:serve": "serve ./dist --cors -p 2300",
    "build-rs": "rslib build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Ken",
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@swc/core": "^1.10.4",
    "@types/node": "^22.10.5"
  }
